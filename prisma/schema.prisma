generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URL")
}

model guildsettings {
  id            String  @id @map("_id")
  alwaysOn      Boolean
  defaultVolume Int
}

model lavalinknodes {
  id     String  @id @default(auto()) @map("_id") @db.ObjectId
  auth   String
  name   String
  secure Boolean
  url    String
}
